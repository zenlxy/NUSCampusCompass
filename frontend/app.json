{
  "expo": {
    "name": "NUSCampusCompass",
    "slug": "NUSCampusCompass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jpowcm.NUSCampusCompass"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jpowcm.NUSCampusCompass"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow NUSCampusCompass to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "68068a48-1ce8-4952-85cc-20060eea0834"
      }
    },
    "owner": "jpowcm",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/68068a48-1ce8-4952-85cc-20060eea0834"
    }
  }
}